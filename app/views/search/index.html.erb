<%= form_tag result_path, method: :get do %>

  <div class="search-box">
    <%= text_field_tag "search", nil, class: "search-txt", placeholder: "Type to search" %>
    <button type="submit" class="search-btn">
    <i class="fa fa-search" aria-hidden="true"></i>
    </button>
  </div> 

<% end %>


